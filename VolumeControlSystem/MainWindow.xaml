<Window
    x:Class="VolumeControlSystem.MainWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:enableDisable="clr-namespace:VolumeControlSystem.UserControls"
    xmlns:local="clr-namespace:VolumeControlSystem"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    Title="MainWindow"
    Width="800"
    Height="450"
    WindowState="Maximized"
    mc:Ignorable="d">
    <Grid>
        <enableDisable:EnableDisableUserControl x:Name="enableDisableUserControl" />
    </Grid>
</Window>
